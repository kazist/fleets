    "query": {
        "from": {
            "table": "Fleets\\Vehicles\\Code\\Tables\\Vehicles",
            "alias": "fv",
            "name": "#__fleets_vehicles"
        },
        "select": [
            "fv.id",
            "fv.registration_no",
            "fv.make_id",
            "fm.title AS make_id_title",
            "fv.model_id",
            "fv.chassis_no",
            "fv.odometer",
            "fv.purchase_date",
            "fv.doors",
            "fv.seats",
            "fv.color",
            "fv.created_by",
            "fv.date_created",
            "fv.modified_by",
            "fv.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Fleets\\Makes\\Code\\Tables\\Makes",
                "alias": "fm",
                "name": "#__fleets_makes",
                "on": "fv.make_id = fm.id"
            },
            {
                "table": "Fleets\\Makes\\Models\\Code\\Tables\\Models",
                "alias": "fmm",
                "name": "#__fleets_makes_models",
                "on": "fv.model_id = fmm.id"
            }
        ]
    }

    "query": {
        "from": {
            "table": "Fleets\\Drivers\\Code\\Tables\\Drivers",
            "alias": "fd",
            "name": "#__fleets_drivers"
        },
        "select": [
            "fd.id",
            "fd.name",
            "fd.phone",
            "fd.email",
            "fd.address",
            "fd.image",
            "mm.title AS image_title",
            "mm.file AS image_file",
            "fd.user_id",
            "uu.name AS user_id_name",
            "fd.notes",
            "fd.documents",
            "mm_3616.title AS documents_title",
            "mm_3616.file AS documents_file",
            "fd.created_by",
            "fd.date_created",
            "fd.modified_by",
            "fd.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Media\\Media\\Code\\Tables\\Media",
                "alias": "mm",
                "name": "#__media_media",
                "on": "fd.image = mm.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu",
                "name": "#__users_users",
                "on": "fd.user_id = uu.id"
            },
            {
                "table": "Media\\Media\\Code\\Tables\\Media",
                "alias": "mm_3616",
                "name": "#__media_media",
                "on": "fd.documents = mm_3616.id"
            }
        ]
    }
